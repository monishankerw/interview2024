           public class CheckTwoArrayAreEqual {
               public static void main(String[] args) {
                   int a[] = {1, 2, 5, 4, 0};
                   int b[] = {2, 4, 5, 0, 1};
                   int n = 5;
                   boolean result = checkTwoArrayAreEqual(a, b, n);
                   System.out.println(result);
               }

               private static boolean checkTwoArrayAreEqual(int[] a, int[] b, int n) {
                   Arrays.sort(a);
                   Arrays.sort(b);
                   for (int i = 0; i < n; i++) {
                       if (a[i] != b[i]) {
                           return false;
                       }
                   }
                   return true;
               }
           }

       1. BUBBLE SORT
       https://www.scaler.com/topics/data-structures/bubble-sort/
       package com.customerService.programmingQuest.sorting;
       public class BubbleSort {
           public static void main(String[] args) {
               int arr[] = {3, 4, 1, 2, 5};
               int n = arr.length;
               bubbleSort(arr, n);
               System.out.println("Bubble sorted array:");
               for (int num : arr) {
                   System.out.print(num + " ");
               }
           }

           private static void bubbleSort(int[] arr, int n) {
               for (int i = 0; i < n - 1; i++) {
                   for (int j = 0; j < n - i - 1; j++) {
                       if (arr[j] > arr[j + 1]) {
                           int temp = arr[j];
                           arr[j] = arr[j + 1];
                           arr[j + 1] = temp;
                       }
                   }
               }
           }
       }

